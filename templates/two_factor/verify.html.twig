{% extends 'base.html.twig' %}

{% block content %}
    <h1>Two-factor verification</h1>

    {% for label, messages in app.flashes %}
        {% for message in messages %}
            <div class="alert alert-{{ label }}" role="alert">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}

    {{ form(form) }}
{% endblock %}